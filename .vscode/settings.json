{
    "python.analysis.extraPaths": [
        "./tools"
    ],
    "python.analysis.exclude": [
        "**/.git",
        "**/.venv*",
        "**/__pycache__",
        "**/.pytest_cache",
        "**/node_modules",
        "**/.DS_Store",
        "**/runs/**",
        "**/notebooks/**",
        "**/analyse/reports/**",
        "**/tools/token.json",
        "**/tools/credentials.json",
        "**/*.md",
        "**/*.json",
        "**/*.txt",
        "**/*.yml",
        "**/*.yaml",
        "**/*.env*",
        "**/*.log",
        "**/*.csv",
        "**/*.xml",
        "**/*.html",
        "**/*.css",
        "**/*.js",
        "**/*.ts",
        "**/.file_cache/**",
        "**/detections.json",
        "**/tracks.json",
        "**/*.pth",
        "**/*.pt",
        "**/*.ckpt",
        "**/*.pkl",
        "**/*.pickle",
        "**/*.ipynb",
        "**/*.jpg",
        "**/*.jpeg",
        "**/*.png",
        "**/*.gif",
        "**/*.bmp",
        "**/*.tiff",
        "**/*.mp4",
        "**/*.avi",
        "**/*.mov",
        "**/*.mkv",
        "**/*.webm"
    ],
    "python.analysis.diagnosticMode": "openFilesOnly"
}